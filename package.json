{
  "name": "nuxt-components",
  "version": "0.0.1",
  "description": "The basic UI component for Nuxt 3",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vizzcaindra/nuxt-ui.git"
  },
  "keywords": [
    "Nuxt",
    "Nuxt3",
    "Component",
    "UI"
  ],
  "author": "vizzcaindra@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vizzcaindra/nuxt-ui/issues"
  },
  "homepage": "https://github.com/vizzcaindra/nuxt-ui#readme",
  "dependencies": {
    "@nuxt/kit": "^3.0.0-rc.12"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.2.0",
    "@nuxt/schema": "^3.0.0-rc.12",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "eslint": "^8.26.0",
    "nuxt": "^3.0.0-rc.12",
    "typescript": "^4.8.4"
  }
}
